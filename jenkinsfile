node{
 stage('Checkout'){
        checkout([$class: 'GitSCM', branches: [[name: '*/master']], doGenerateSubmoduleConfigurations: false, extensions: [], submoduleCfg: [], userRemoteConfigs: [[url: 'https://github.com/azkiatunnisarf/URL-Shortener-ITS']]])
    }
 stage('Pull latest change'){
	sh 'git pull'
    }
}
