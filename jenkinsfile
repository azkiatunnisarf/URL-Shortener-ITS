node{
 stage('Checkout'){
        checkout([$class: 'GitSCM', branches: [[name: '*/jenkinsfile']], doGenerateSubmoduleConfigurations: false, extensions: [], submoduleCfg: [], userRemoteConfigs: [[url: 'https://github.com/hilmiraditya/ITS-UrlShortener']]])
    }
 stage('Build Image'){
	sh 'git pull'
    }
}
